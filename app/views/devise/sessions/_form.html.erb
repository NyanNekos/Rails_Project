<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <form>
  <div class="mb-3"><label class="form-label" for="email">Email</label><%= f.email_field :email, autofocus: true, autocomplete: "email",class: "form-control item" %></div>
  <div class="mb-3"><label class="form-label" for="password">Password</label><%= f.password_field :password, autocomplete: "current-password",class: "form-control item" %></div>
  <div class="mb-3">
    <% if devise_mapping.rememberable? %>
      <div class="form-check"><%= f.check_box :remember_me, class:"form-check-input " %><label class="form-check-label" for="checkbox">Remember me</label></div>
    <% end %>
  </div>
  <%= f.submit "Log in", class: "btn btn-primary"%>
</form>
<% end %>